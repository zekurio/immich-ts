{
  "name": "immich-ts",
  "module": "src/index.ts",
  "type": "module",
  "private": true,
  "devDependencies": {
    "@types/bun": "^1.3.0",
    "oxlint": "^1.39.0",
    "typescript": "^5"
  },
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "bun build ./src/index.ts --target=bun --compile --outfile ./dist/immich-ts",
    "build:release": "bun build ./src/index.ts --target=bun --compile --bytecode --minify --outfile ./dist/immich-ts",
    "typecheck": "bunx tsc --noEmit",
    "test": "bun test",
    "test:unit": "bun test tests/unit",
    "test:cli": "bun test tests/cli",
    "lint": "oxlint"
  },
  "dependencies": {
    "@immich/sdk": "^2.4.1"
  }
}
